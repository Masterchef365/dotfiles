#define  S_background  #24282c
#define  S_foreground  #c5c8c6
#define  S_color0      #282a2e
#define  S_color1      #e47741
#define  S_color2      #93dc41
#define  S_color3      #ff0086
#define  S_color4      #16a8e3
#define  S_color5      #ad41c9
#define  S_color6      #c0e224
#define  S_color7      #afb7c4
#define  S_color8      #646970
#define  S_color9      #e47741
#define  S_color10     #93dc41
#define  S_color11     #ff0086
#define  S_color12     #16a8e3
#define  S_color13     #ad41c9
#define  S_color14     #c0e224
#define  S_color15     #c5c8c6

! ---------------------------- ROFI -------------------------------
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg        hlfg
rofi.color-window:  S_background,  S_background
rofi.color-normal:  S_background,  S_foreground,  S_background,  S_background,  S_color10
rofi.color-active:  S_background,  S_foreground,  S_background,  S_background,  S_color10
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_background,  S_color10
rofi.opacity: 100
rofi.font: Fantasque Sans Mono 13
rofi.dpi: 175
rofi.location: 1
rofi.xoffset: 0
rofi.terminal: alacritty

! ---------------------------- URXVT -------------------------------

! special
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground

!black
*color0:       S_color0
*color8:       S_color8

!red
*color1:       S_color1
*color9:       S_color9

!green
*color2:       S_color2
*color10:      S_color10

!yellow
*color3:       S_color3
*color11:      S_color11

!blue
*color4:       S_color4
*color12:      S_color12

!magenta
*color5:       S_color5
*color13:      S_color13

!cyan
*color6:       S_color6
*color14:      S_color14

!white
*color7:       S_color7
*color15:      S_color15

Xft.dpi: 175

!urxvt*transparent: true
!urxvt*shading: 10
!urxvt*tintColor: #FFFFFFFF
!urxvt*blurRadius: 30

!urxvt*background: rgba:2828/2828/2828/EEEE
!urxvt*depth: 32
