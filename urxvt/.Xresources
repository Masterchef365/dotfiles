#define  S_foreground  #c3d9d9
#define  S_background  #0b171c
#define  S_color0      #0b171c
#define  S_color8      #487099
#define  S_color1      #a15bb5
#define  S_color9      #f556b5
#define  S_color2      #2bbfd6
#define  S_color10     #2bbfd6
#define  S_color3      #09abe6
#define  S_color11     #09abe6
#define  S_color4      #09abe6
#define  S_color12     #09abe6
#define  S_color5      #a46ab0
#define  S_color13     #a46ab0
#define  S_color6      #ff4fa7
#define  S_color14     #ff4fa7
#define  S_color7      #487099
#define  S_color15     #c3d9d9

! ---------------------------- ROFI -------------------------------
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg        hlfg
rofi.color-window:  S_background,  S_background
rofi.color-normal:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.color-active:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.opacity: 10.0
rofi.font: Fantasque Sans Mono 13
rofi.dpi: 170
rofi.location: 1
!rofi.yoffset: 40
rofi.xoffset: 0
!rofi.width: 2880

! ---------------------------- URXVT -------------------------------

! special
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground

!black
*color0:       S_color0
*color8:       S_color8

!red
*color1:       S_color1
*color9:       S_color9

!green
*color2:       S_color2
*color10:      S_color10

!yellow
*color3:       S_color3
*color11:      S_color11

!blue
*color4:       S_color4
*color12:      S_color12

!magenta
*color5:       S_color5
*color13:      S_color13

!cyan
*color6:       S_color6
*color14:      S_color14

!white
*color7:       S_color7
*color15:      S_color15


URxvt.font: xft:FantasqueSansMono Nerd Font
!URxvt.font: xft:terminus-12:encoding=combined
URxvt*scrollBar: false
URxvt.internalBorder: 6
Xft.dpi: 175

!urxvt*transparent: true
!urxvt*shading: 10
!urxvt*tintColor: #FFFFFFFF
!urxvt*blurRadius: 30

!urxvt*background: rgba:2828/2828/2828/EEEE
!urxvt*depth: 32
