#define S_foreground 	#ffffff
#define S_background 	#343c24
#define S_color0 	#2e2e2e
#define S_color8 	#4b7200
#define S_color1 	#5d7262
#define S_color9 	#71705e
#define S_color2 	#717f4f
#define S_color10 	#698d23
#define S_color3 	#81866c
#define S_color11 	#999c89
#define S_color4 	#788822
#define S_color12 	#71a60d
#define S_color5 	#727857
#define S_color13 	#819341
#define S_color6 	#558400
#define S_color14 	#8ca525
#define S_color7 	#267c08
#define S_color15 	#868573

Xft.dpi: 160
! ---------------------------- ROFI --------------------------------

rofi.color-enabled: true
! State:				  bg					fg					bgalt				hlbg			hlfg
rofi.color-window:  S_background,	S_background
rofi.color-normal:  S_background, 	S_foreground,	S_background,	S_color8,	S_foreground
rofi.color-active:  S_background, 	S_foreground,	S_background,	S_color8,	S_foreground
rofi.color-urgent:  S_background, 	S_foreground,	S_background,	S_color8,	S_foreground
rofi.opacity: 87.5
rofi.font: xft:Fantasque Sans Mono Regular Nerd Font Complete Mono 11
rofi.dpi: 170

! ---------------------------- URXVT -------------------------------

! special
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground

!black
*color0:       S_color0
*color8:       S_color8

!red
*color1:       S_color1
*color9:       S_color9

!green
*color2:       S_color2
*color10:      S_color10

!yellow
*color3:       S_color3
*color11:      S_color11

!blue
*color4:       S_color4
*color12:      S_color12

!magenta
*color5:       S_color5
*color13:      S_color13

!cyan
*color6:       S_color6
*color14:      S_color14

!white
*color7:       S_color7
*color15:      S_color15


URxvt.font: xft:FantasqueSansMono Nerd Font
URxvt*scrollBar: false
urxvt*depth: 32
urxvt*transparent: true
urxvt*shading: 15

