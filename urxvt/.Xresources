Xft.dpi: 160
! ---------------------------- ROFI --------------------------------

rofi.color-enabled: true
! State:				  bg					fg					bgalt				hlbg			hlfg
rofi.color-window:  S_background,	S_background
rofi.color-normal:  S_background, 	S_foreground,	S_background,	S_color8,	S_foreground
rofi.color-active:  S_background, 	S_foreground,	S_background,	S_color8,	S_foreground
rofi.color-urgent:  S_background, 	S_foreground,	S_background,	S_color8,	S_foreground
rofi.opacity: 87.5
rofi.font: xft:Fantasque Sans Mono Regular Nerd Font Complete Mono 11
rofi.dpi: 170

! ---------------------------- URXVT -------------------------------

! special
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground

!black
*color0:       S_color0
*color8:       S_color8

!red
*color1:       S_color1
*color9:       S_color9

!green
*color2:       S_color2
*color10:      S_color10

!yellow
*color3:       S_color3
*color11:      S_color11

!blue
*color4:       S_color4
*color12:      S_color12

!magenta
*color5:       S_color5
*color13:      S_color13

!cyan
*color6:       S_color6
*color14:      S_color14

!white
*color7:       S_color7
*color15:      S_color15


URxvt.font: xft:FantasqueSansMono Nerd Font
URxvt*scrollBar: false
urxvt*depth: 32
URxvt.internalBorder: 6

!urxvt*transparent: true
!urxvt*shading: 60
!urxvt*background: rgba:1111/1111/1111/cccc
