#define  S_foreground  #ffffff
#define  S_background  #282828
#define  S_color0      #282828
#define  S_color8      #6f6f6f
#define  S_color1      #ff00cc
#define  S_color9      #ff00cc
#define  S_color2      #ff28a3
#define  S_color10     #ff28a3
#define  S_color3      #ff517a
#define  S_color11     #ff517a
#define  S_color4      #ff7a51
#define  S_color12     #ff7a51
#define  S_color5      #ffa328
#define  S_color13     #ffa328
#define  S_color6      #ffcc00
#define  S_color14     #ffcc00
#define  S_color7      #aeaeae
#define  S_color15     #ffffff

! ---------------------------- ROFI -------------------------------
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg        hlfg
rofi.color-window:  S_background,  S_background
rofi.color-normal:  S_background,  S_foreground,  S_background,  S_background,  S_color10
rofi.color-active:  S_background,  S_foreground,  S_background,  S_background,  S_color10
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_background,  S_color10
rofi.opacity: 100
rofi.font: Fantasque Sans Mono 13
rofi.dpi: 175
rofi.location: 1
rofi.xoffset: 0
rofi.terminal: alacritty

! ---------------------------- URXVT -------------------------------

! special
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground

!black
*color0:       S_color0
*color8:       S_color8

!red
*color1:       S_color1
*color9:       S_color9

!green
*color2:       S_color2
*color10:      S_color10

!yellow
*color3:       S_color3
*color11:      S_color11

!blue
*color4:       S_color4
*color12:      S_color12

!magenta
*color5:       S_color5
*color13:      S_color13

!cyan
*color6:       S_color6
*color14:      S_color14

!white
*color7:       S_color7
*color15:      S_color15

Xft.dpi: 175

!urxvt*transparent: true
!urxvt*shading: 10
!urxvt*tintColor: #FFFFFFFF
!urxvt*blurRadius: 30

!urxvt*background: rgba:2828/2828/2828/EEEE
!urxvt*depth: 32
