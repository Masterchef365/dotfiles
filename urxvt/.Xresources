rofi.color-active:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.opacity: 10.0
rofi.font: Fantasque Sans Mono 13
rofi.dpi: 170
rofi.location: 1
!rofi.yoffset: 40
rofi.xoffset: 0
!rofi.width: 2880

