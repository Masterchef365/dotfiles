#define  S_foreground  #ffffff
#define  S_background  #1f1f1f
#define  S_color0      #1f1f1f
#define  S_color8      #9ac383
#define  S_color1      #548538
#define  S_color9      #548538
#define  S_color2      #7bc352
#define  S_color10     #7bc352
#define  S_color3      #b5ea97
#define  S_color11     #b5ea97
#define  S_color4      #7bc352
#define  S_color12     #7bc352
#define  S_color5      #548538
#define  S_color13     #548538
#define  S_color6      #b5ea97
#define  S_color14     #b5ea97
#define  S_color7      #5f7750
#define  S_color15     #ffffff

! ---------------------------- ROFI -------------------------------
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg        hlfg
rofi.color-window:  S_background,  S_background
rofi.color-normal:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.color-active:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_color10,  S_color0
rofi.opacity: 10.0
rofi.font: Fantasque Sans Mono 13
rofi.dpi: 175
rofi.location: 1
!rofi.yoffset: 40
rofi.xoffset: 0
!rofi.width: 2880

! ---------------------------- URXVT -------------------------------

! special
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground

!black
*color0:       S_color0
*color8:       S_color8

!red
*color1:       S_color1
*color9:       S_color9

!green
*color2:       S_color2
*color10:      S_color10

!yellow
*color3:       S_color3
*color11:      S_color11

!blue
*color4:       S_color4
*color12:      S_color12

!magenta
*color5:       S_color5
*color13:      S_color13

!cyan
*color6:       S_color6
*color14:      S_color14

!white
*color7:       S_color7
*color15:      S_color15


URxvt.font: xft:FantasqueSansMono Nerd Font
URxvt*scrollBar: false
URxvt.internalBorder: 6
Xft.dpi: 175

!urxvt*transparent: true
!urxvt*shading: 10
!urxvt*tintColor: #FFFFFFFF
!urxvt*blurRadius: 30

!urxvt*background: rgba:2828/2828/2828/EEEE
!urxvt*depth: 32
