! Color definitions
#define S_background #1b1d1a
#define S_foreground #e5dad3

#define S_color0    #252824
#define S_color1    #ba4e4e
#define S_color2    #8aa66e
#define S_color3    #d3b265
#define S_color4    #61a9b4
#define S_color5    #c2865c
#define S_color6    #6f8558
#define S_color7    #e5dad3

#define S_color8    #6e6859
#define S_color9    #ba4e4e
#define S_color10   #8aa66e
#define S_color11   #d3b265
#define S_color12   #61a9b4
#define S_color13   #c2865c
#define S_color14   #6f8558
#define S_color15   #e5dad3

#define S_DPI 220

! Rofi Settings
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg           hlfg
rofi.color-window:  S_background,  S_color11                                    
rofi.color-normal:  S_color0,      S_foreground,  S_color0,      S_color11,     S_background
rofi.color-active:  S_color0,      S_foreground,  S_color0,      S_background,  S_foreground
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_background,  S_foreground
rofi.font:          Fantasque Sans Mono 11             
rofi.dpi:           S_DPI
rofi.location:      0                                                           
rofi.xoffset:       0                                                           
rofi.terminal:      alacritty                                                   

! URXVT/Xterm colors
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground
*color0:       S_color0
*color1:       S_color1
*color2:       S_color2
*color3:       S_color3
*color4:       S_color4
*color5:       S_color5
*color6:       S_color6
*color7:       S_color7
*color8:       S_color8
*color9:       S_color9
*color10:      S_color10
*color11:      S_color11
*color12:      S_color12
*color13:      S_color13
*color14:      S_color14
*color15:      S_color15

! Xorg settings
Xft.dpi:       S_DPI
