! Color definitions
#define S_background #010e11
#define S_foreground #e8dfd6

#define S_color0    #032c36
#define S_color1    #c2454e
#define S_color2    #5dbf8f
#define S_color3    #8a7a63
#define S_color4    #2a3040
#define S_color5    #ff5879
#define S_color6    #44b5b1
#define S_color7    #f2f1ae

#define S_color8    #065f73
#define S_color9    #ef5847
#define S_color10   #84d99b
#define S_color11   #beaa7d
#define S_color12   #4e6e8d
#define S_color13   #ff99a1
#define S_color14   #74d9d7
#define S_color15   #e8dfd6

#define S_DPI 96

! Rofi Settings
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg           hlfg
rofi.color-window:  S_background,  S_color11                                    
rofi.color-normal:  S_color0,      S_foreground,  S_color0,      S_color11,     S_background
rofi.color-active:  S_color0,      S_foreground,  S_color0,      S_background,  S_foreground
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_background,  S_foreground
rofi.font:          Monospace 12             
rofi.dpi:           S_DPI
rofi.location:      0                                                           
rofi.xoffset:       0                                                           
rofi.terminal:      alacritty                                                   

! URXVT/Xterm colors
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground
*color0:       S_color0
*color1:       S_color1
*color2:       S_color2
*color3:       S_color3
*color4:       S_color4
*color5:       S_color5
*color6:       S_color6
*color7:       S_color7
*color8:       S_color8
*color9:       S_color9
*color10:      S_color10
*color11:      S_color11
*color12:      S_color12
*color13:      S_color13
*color14:      S_color14
*color15:      S_color15

! Xorg settings
Xft.dpi:       S_DPI
