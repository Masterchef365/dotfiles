! Color definitions
#define S_background #141210
#define S_foreground #ffffff

#define S_color0    #141210
#define S_color1    #8ecc30
#define S_color2    #fd9000
#define S_color3    #ffd100
#define S_color4    #ef488b
#define S_color5    #e45f49
#define S_color6    #8ecc30
#define S_color7    #dbdbdb

#define S_color8    #645348
#define S_color9    #8ecc30
#define S_color10   #fd9000
#define S_color11   #ffd100
#define S_color12   #ef9048
#define S_color13   #e45f49
#define S_color14   #8ecc30
#define S_color15   #ffffff

#define S_DPI 220

! Rofi Settings
rofi.color-enabled: true
!State:             bg             fg             bgalt          hlbg           hlfg
rofi.color-window:  S_background,  S_color11                                    
rofi.color-normal:  S_color0,      S_foreground,  S_color0,      S_color11,     S_background
rofi.color-active:  S_color0,      S_foreground,  S_color0,      S_background,  S_foreground
rofi.color-urgent:  S_background,  S_foreground,  S_background,  S_background,  S_foreground
rofi.font:          Fantasque Sans Mono 11             
rofi.dpi:           S_DPI
rofi.location:      0                                                           
rofi.xoffset:       0                                                           
rofi.terminal:      alacritty                                                   

! URXVT/Xterm colors
*foreground:   S_foreground
*background:   S_background
*cursorColor:  S_foreground
*color0:       S_color0
*color1:       S_color1
*color2:       S_color2
*color3:       S_color3
*color4:       S_color4
*color5:       S_color5
*color6:       S_color6
*color7:       S_color7
*color8:       S_color8
*color9:       S_color9
*color10:      S_color10
*color11:      S_color11
*color12:      S_color12
*color13:      S_color13
*color14:      S_color14
*color15:      S_color15

! Xorg settings
Xft.dpi:       S_DPI
